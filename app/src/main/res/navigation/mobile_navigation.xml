<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="mr_garuda.sportsmate.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_match"
        android:name="mr_garuda.sportsmate.ui.match.MatchFragment"
        android:label="@string/title_match"
        tools:layout="@layout/fragment_match" />

    <fragment
        android:id="@+id/navigation_myprofile"
        android:name="mr_garuda.sportsmate.ui.myProfile.MyProfileFragment"
        android:label="@string/title_myprofile"
        tools:layout="@layout/fragment_myprofile" />

    <fragment
        android:id="@+id/navigation_more"
        android:name="mr_garuda.sportsmate.ui.more.MoreFragment"
        android:label="@string/title_more"
        tools:layout="@layout/fragment_more" />
</navigation>